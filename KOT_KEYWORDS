// A '#' means a special statement that can ignore semantics,
// run before runtime (or condiationally during it),
// or setup a special type that cannot be defined.
#require
#config
#use

#triplet
#os
#family
#arch

#comptime
#testtime

#bytes
#map
#set
#regex

0x // hex
0b // binary

const
let
var

? // optional

true
false

// No Special Case
bool
int
-uint
float
char
string
any
-void = ()

range
regex

map
set
//

